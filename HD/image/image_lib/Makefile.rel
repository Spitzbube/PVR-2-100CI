LIBRARY = libimage

HEADER_SUBDIR = image

HEADERS += include/*.h
################################################################################
# define local file to be installed
################################################################################

COPYFLAGS_USER += -r

################################################################################
# define user targets
################################################################################

default: install

clean: 

build: install

headers: install-headers

install: install-library-released install-headers

all: install


